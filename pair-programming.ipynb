    "import numpy as np\n",
    "def data_mean_offset(data,change_mean):\n",
    "    data_mean = np.mean(data)\n",
    "    diff = change_mean - data_mean\n",
    "    data = data + diff\n",
    "    return (data)\n",
    "data = np.array([1,2,3,4,5])\n",
    "print(data_mean_offset(data,2))\n",
    "np.mean(data_mean_offset(data,2))\n",
    "\n",
    "        "
   ]
  }
